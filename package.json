{
  "name": "gatsby-starter-blog",
  "private": true,
  "description": "A starter for a blog powered by Gatsby and Markdown",
  "version": "0.1.0",
  "author": "Abhishek Sah",
  "bugs": {
    "url": "https://github.com/whoAbhishekSah/whoAbhishekSah"
  },
  "dependencies": {
    "disqus-react": "^1.1.5",
    "gatsby": "^5.10.0",
    "gatsby-image": "^2.3.5",
    "gatsby-plugin-feed": "^5.10.0",
    "gatsby-plugin-google-analytics": "^5.10.0",
    "gatsby-plugin-manifest": "^5.10.0",
    "gatsby-plugin-offline": "^6.10.0",
    "gatsby-plugin-react-helmet": "^6.10.0",
    "gatsby-plugin-sharp": "^5.10.0",
    "gatsby-plugin-typography": "^5.10.0",
    "gatsby-remark-copy-linked-files": "^6.10.0",
    "gatsby-remark-embed-gist": "^1.2.1",
    "gatsby-remark-images": "^7.10.0",
    "gatsby-remark-prismjs": "^7.10.0",
    "gatsby-remark-responsive-iframe": "^6.10.0",
    "gatsby-remark-smartypants": "^6.10.0",
    "gatsby-source-filesystem": "^5.10.0",
    "gatsby-transformer-remark": "^6.10.0",
    "gatsby-transformer-sharp": "^5.10.0",
    "gh-pages": "^5.0.0",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.9.0",
    "react-typography": "^0.16.23",
    "typeface-merriweather": "1.1.13",
    "typeface-montserrat": "1.1.13",
    "typography": "^0.16.21",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "prettier": "2.8.8"
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 80
  },
  "eslintConfig": {
    "extends": "./config/.eslintrc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint"
    }
  },
  "homepage": "https://github.com/whoAbhishekSah/whoAbhishekSah.github.io",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "https://github.com/whoAbhishekSah/whoAbhishekSah.github.io"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "./node_modules/prettier/bin-prettier.js --check \"**/*.{js,jsx,json,md}\"",
    "format:fix": "yarn format --write",
    "start": "npm run develop",
    "lint": "./node_modules/.bin/eslint .",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public -b master"
  }
}
